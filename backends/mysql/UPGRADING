0.5.x to 0.5.4

-- uid and gid should be unique
ALTER TABLE groups MODIFY gid INT UNSIGNED NOT NULL UNIQUE;
ALTER TABLE users MODIFY uid INT(10) UNSIGNED NOT NULL UNIQUE;

-- rename tables to lowercase
ALTER TABLE GroupIP RENAME TO groupip;
ALTER TABLE Stats RENAME TO stats;
ALTER TABLE UGR RENAME TO ugr;
ALTER TABLE UserIP RENAME TO userip;
